{
  "name": "unstorage-driver-query-string",
  "version": "1.2.0",
  "description": "Query string driver for unstorage - store state in URL parameters",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest --run --config vitest.config.mjs",
    "test:ci": "vitest --run --config vitest.config.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "release": "release-it"
  },
  "keywords": [
    "unstorage",
    "driver",
    "query-string",
    "url",
    "state",
    "browser"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "@types/history": "^5.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.5.2",
    "@types/qs": "^6.14.0",
    "@types/validator": "^13.15.3",
    "eslint": "^9.36.0",
    "eslint-config-agent": "^1.3.7",
    "eslint-plugin-required-exports": "^0.2.0",
    "eslint-plugin-single-export": "^1.1.2",
    "globals": "^16.4.0",
    "happy-dom": "^18.0.1",
    "release-it": "^19.0.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "es-toolkit": "1.39.10",
    "history": "^5.3.0",
    "qs": "^6.14.0",
    "tiny-invariant": "^1.3.3",
    "unstorage": "^1.17.1",
    "validator": "^13.15.15"
  }
}
